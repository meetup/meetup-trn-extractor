{
  "name": "meetup-trn-extractor",
  "version": "0.2.0",
  "description": "A utility to extract copy from JavaScript files for translation",
  "main": "lib/extractor.js",
  "bin": {
    "meetup-trn-extractor": "lib/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikespencer/meetup-trn-extractor.git"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "rm -rf node_modules lib coverage",
    "coverage": "istanbul cover _mocha -- --require babel-register",
    "flow": "flow check",
    "lint": "eslint src test",
    "prepublish": "rm -rf lib/* && npm run build",
    "test": "_mocha --require babel-register"
  },
  "keywords": [
    "Meetup",
    "i18n",
    "translation",
    "translate",
    "trn",
    "javascript",
    "js",
    "babylon"
  ],
  "author": "Mike Spencer <mspencer@meetup.com>",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.18.2",
    "babel-traverse": "^6.19.0",
    "babylon": "^6.14.0",
    "glob": "^7.1.1",
    "yargs": "^6.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.0",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.15",
    "eslint": "^3.10.2",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-flowtype": "^2.25.0",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-standard": "^2.0.1",
    "flow-bin": "^0.35.0",
    "istanbul": "^1.0.0-alpha",
    "mocha": "^3.1.2",
    "sinon": "^1.17.6"
  }
}
